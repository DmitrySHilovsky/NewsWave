{
    "name": "tms-auto",
    "version": "1.0.0",
    "description": "TeachMeSkills - Automation QA",
    "main": "index.js",
    "scripts": {
        "hw16": "ts-node homework_16/src/index.ts",
        "hw16-watch": "nodemon --config homework_16/nodemon.json homework_16/src/index.ts",
        "hw17-mocha": "mocha --config=homework_17/.mocharc.json",
        "hw17-jest": "jest --config=homework_17/jest.config.json",
        "hw17-mocha-nyc": "nyc npm run hw17-mocha",
        "hw18": "jest --config=homework_18/jest.config.json",
        "hw19": "mocha --config=homework_19/.mocharc.json --timeout 40000",
        "hw21": "mocha --config=homework_21/.mocharc.json --timeout 40000",
        "hw22": "wdio run homework_22/wdio.conf.ts",
        "hw22-allure": "allure generate homework_22/assets/reports/allure-report --clean -o homework_22/assets/reports/allure-result && allure open homework_22/assets/reports/allure-result",
        "hw23": "playwright test --config=homework_23/configs/playwright.config.ts",
        "hw23-allure": "allure generate homework_23/assets/reports/allure-report --clean -o homework_23/assets/reports/allure-result && allure open homework_23/assets/reports/allure-result",
        "hw24-cy:open": "cypress open --config-file=homework_24/cypress/cypress.config.ts",
        "hw24-cy:run": "cypress run --browser chrome --headed --config-file=homework_24/cypress/cypress.config.ts",
        "hw24-cy:allure": "allure generate homework_24/cypress/assets/reporter/allure-report --clean -o homework_24/cypress/assets/reporter/allure-result && allure open homework_24/cypress/assets/reporter/allure-result"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vladimirchizhenkov/tmsauto.git"
    },
    "keywords": [
        "qa",
        "authomatization",
        "cypress",
        "wdio",
        "mocha",
        "jest"
    ],
    "author": "Vladimir Chizhenkov",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/vladimirchizhenkov/tmsauto/issues"
    },
    "homepage": "https://github.com/vladimirchizhenkov/tmsauto#readme",
    "devDependencies": {
        "@cypress/xpath": "^2.0.3",
        "@playwright/test": "^1.27.0",
        "@shelex/cypress-allure-plugin": "^2.31.0",
        "@types/chai": "^4.3.1",
        "@types/jest": "^28.1.6",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.6.4",
        "@types/selenium-webdriver": "^4.1.2",
        "@types/superagent": "^4.1.15",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "@wdio/cli": "^7.23.0",
        "@wdio/cucumber-framework": "^7.23.0",
        "@wdio/local-runner": "^7.23.0",
        "@wdio/spec-reporter": "^7.23.0",
        "allure-commandline": "^2.18.1",
        "allure-playwright": "^2.0.0-beta.19",
        "chai": "^4.3.6",
        "chromedriver": "^104.0.0",
        "cypress": "^10.8.0",
        "eslint": "^8.21.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "jest": "^28.1.3",
        "log4js": "^6.7.0",
        "mocha": "^10.0.0",
        "nodemon": "^2.0.19",
        "nyc": "^15.1.0",
        "selenium-webdriver": "^4.4.0",
        "superagent": "^8.0.0",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.9.1",
        "tslib": "^1.14.1",
        "typescript": "^4.7.4",
        "wdio-chromedriver-service": "^7.3.2"
    },
    "dependencies": {
        "@wdio/allure-reporter": "^7.23.0",
        "playwright-webkit": "^1.25.2"
    }
}
